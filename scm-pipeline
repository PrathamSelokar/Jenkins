pipeline {
    agent any

    stages {
        stage('git-pull') {
            steps {
                git branch: 'main',
                    credentialsId: 'ssh-cred',
                    url: 'https://github.com/PrathamSelokar/Jenkins.git'
            }
        }

        stage('Build') {
            steps {
                echo "build-success"
            }
        }

        stage('Test') {
            steps {
                echo "test-success"
            }
        }

        stage('Deploy') {
            steps {
                echo "deploy-success"
            }
        }
    }
}
