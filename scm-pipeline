pipeline {
    agent any
    stages {
        stage('git-pull') {
           git branch: 'main', credentialsId: 'ssh-cred', url: 'https://github.com/PrathamSelokar/Jenkins.git'
            }
        }
        stage('Build') {
            steps {
                echo "build-sucess"
            }
        }
        stage('Test') {
            steps {
                echo "test sucess"
            }
        }
        stage('Deploy') {
            steps {
                echo "deploy sucess"
            }
        }
    }

}

