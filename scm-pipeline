pipeline {
    agent any
    stages {
        stage('git-pull') {
            steps {git branch: '*/main', url: 'https://github.com/PrathamSelokar/Jenkins.git'
            }
        }
        stage('Build') {
            steps {
                echo "build-sucess"
            }
        }
        stage('Test') {
            steps {
                echo "test sucess"
            }
        }
        stage('Deploy') {
            steps {
                echo "deploy sucess"
            }
        }
    }

}
